<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pro_restauranttool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>


        <activity android:name=".MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MENU" />

            </intent-filter>
        </activity>

        <activity android:name=".ReservationActivity">
            <intent-filter>
                <action android:name="android.intent.action.RESERVATION" />

            </intent-filter>
        </activity>

        <activity android:name=".SeatActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEAT" />

            </intent-filter>
        </activity>


        <activity android:name=".StatisticActivity">
            <intent-filter>
                <action android:name="android.intent.action.STATISTIC" />

            </intent-filter>
        </activity>


        <activity android:name=".tablesActivity">
            <intent-filter>
                <action android:name="android.intent.action.TABLES" />

            </intent-filter>
        </activity>



    </application>

</manifest>